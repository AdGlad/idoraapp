<p id="notice"><%= notice %></p>

<h1>Identities</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Name</th>
      <th>Picture</th>
      <th>Dob</th>
      <th>Team</th>
      <th>Desc</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @identities.each do |identity| %>
      <tr>
        <td><%= identity.user %></td>
        <td><%= identity.name %></td>
        <td><%= identity.picture %></td>
        <td><%= identity.dob %></td>
        <td><%= identity.team %></td>
        <td><%= identity.desc %></td>
        <td><%= link_to 'Show', identity %></td>
        <td><%= link_to 'Edit', edit_identity_path(identity) %></td>
        <td><%= link_to 'Destroy', identity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Identity', new_identity_path %>
